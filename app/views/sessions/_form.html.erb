<%= form_for @session do |f| %>
  <% if f.object.errors.any? %>
    <div id="error_explanation">
      <ul>
      <% f.object.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :new %><br>
    <%= f.text_field :new %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
